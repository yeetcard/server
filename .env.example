# Server
PORT=3000
NODE_ENV=development

# Apple Certificate
PASS_TYPE_IDENTIFIER=pass.com.yourcompany.yeetcard
TEAM_IDENTIFIER=YOUR_TEAM_ID
CERTIFICATE_PATH=./certs/pass.p12
CERTIFICATE_PASSWORD=your-certificate-password
WWDR_CERTIFICATE_PATH=./certs/AppleWWDRCA.pem

# Authentication
API_KEYS=dev-key-1,dev-key-2

# Rate Limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW_MS=3600000

# Logging
LOG_LEVEL=info
